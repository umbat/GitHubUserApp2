<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.githubuserapp2">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GitHubUserApp2">
        <activity
            android:name="com.example.githubuserapp2.ui.darkmode.DarkModeActivity"
            android:exported="false" />
        <activity
            android:name=".favorite.FavoriteActivity"
            android:exported="false" />
        <activity
            android:name=".detail.DetailUserActivity"
            android:exported="false" />
        <activity
            android:name=".list.ListUserActivity"
            android:exported="false"
            android:parentActivityName=".main.MainActivity" />
        <activity
            android:name="com.example.githubuserapp2.ui.otheractivity.HelpActivity"
            android:exported="false"
            android:parentActivityName=".main.MainActivity" />
        <activity
            android:name="com.example.githubuserapp2.ui.otheractivity.AboutActivity"
            android:exported="false"
            android:parentActivityName=".main.MainActivity" />
        <activity
            android:name="com.example.githubuserapp2.ui.otheractivity.SplashActivity"
            android:exported="true"
            android:theme="@style/NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".main.MainActivity"
            android:exported="false" />
    </application>

</manifest>